{
  "subscriptions": [
    {
      "name": "maven",
      "url": "http://maven.apache.org/download.cgi",
      "cache": {
        "url": "http://www.github.com/lifuzu/subnotify.git",
        "scope": "div.section",
        "exclude": "#SelectMirror",
        "file": "records/maven.cache"
      },
      "criterion": [{
        "url": "http://www.github.com/lifuzu/subnotify.git",
        "selector": "div.section h2",
        "pattern": "\\d+.\\d+.\\d+",
        "file": "records/maven.criteria"
      }],
      "patterns": [
        {
          "name": "version",
          "type": "string",
          "selector": "div.section h2",
          "pattern": "\\d+.\\d+.\\d+"
        },
        {
          "name": "package",
          "type": "zip",
          "selector": "href",
          "pattern": "apache-maven-{version}-bin.zip"
        },
        {
          "name": "checksum",
          "type": "md5",
          "selector": "href",
          "pattern" : "{package}.md5"
        }
      ]
    }
  ],
  "notifications": [
    {
      "email" : {
        "to": [
          "rlee@book.com"
        ],
        "template": "templates/notification.tmpl"
      }
    }
  ]
}

